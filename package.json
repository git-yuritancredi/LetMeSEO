{
  "name": "letmeseo",
  "version": "1.0.0",
  "description": "Analyze site SEO",
  "main": "App.js",
  "scripts": {
    "dev": "gulp run",
    "prod": "DEVELOPMENT=1 electron .",
    "build": "gulp build",
    "rebuild": "gulp rebuild",
    "pack": "npm run rebuild && electron-builder build -mlw"
  },
  "build": {
    "appId": "opensource.letmeseo.app",
    "productName": "LetMeSeo",
    "directories": {
      "output": "build"
    },
    "files": [
      "App.js",
      "app/**/*",
      "assets/images/*",
      "locales/*",
      "!gulpfile.js",
      "!.babelrc",
      "!.babelrc",
      "!.gitignore",
      "!letmeseo.db",
      "!components"
    ],
    "compression": "maximum",
    "mac": {
      "category": "public.app-category.developer-tools",
      "icon": "./assets/build/icon.icns",
      "asar": true
    },
    "win": {
      "target": "portable",
      "icon": "./assets/build/icon.icns",
      "publisherName": "Yuri Tancredi"
    },
    "linux": {
      "target": "AppImage",
      "executableName": "LetMeSeo",
      "category": "Development",
      "description": "Analyze site SEO"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/git-yuritancredi/LetMeSEO.git"
  },
  "author": "Yuri Tancredi",
  "license": "CC0-1.0",
  "bugs": {
    "url": "https://github.com/git-yuritancredi/LetMeSEO/issues"
  },
  "homepage": "https://github.com/git-yuritancredi/LetMeSEO#readme",
  "devDependencies": {
    "@babel/core": "^7.14.3",
    "@babel/preset-env": "^7.14.2",
    "@babel/preset-react": "^7.13.13",
    "electron": "^13.1.9",
    "electron-builder": "^22.11.7",
    "gulp": "^4.0.2",
    "gulp-babel": "^8.0.0",
    "gulp-clean-css": "^4.3.0",
    "gulp-concat": "^2.6.1"
  },
  "dependencies": {
    "@material-ui/core": "^4.11.4",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.58",
    "fetch": "^1.1.0",
    "he": "^1.2.0",
    "i18n": "^0.13.3",
    "lokijs": "^1.5.12",
    "node-html-parser": "^4.1.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  }
}
