{
  "name": "letmeseo",
  "version": "1.0.0",
  "description": "Analyze site SEO",
  "main": "App.js",
  "scripts": {
    "dev": "gulp run",
    "prod": "DEVELOPMENT=1 electron .",
    "build": "gulp build",
    "rebuild": "gulp rebuild",
    "pack": "electron-builder build -m -l -w"
  },
  "build": {
    "appId": "opensource.letmeseo.app",
    "productName": "LetMeSeo",
    "directories": {
      "output": "build"
    },
    "files": ["App.js", "app/**/*", "assets/images/*", "!gulpfile.js", "!.babelrc", "!.babelrc", "!.gitignore", "!letmeseo.db", "!components"],
    "compression": "maximum",
    "mac": {
      "category": "public.app-category.developer-tools",
      "icon": "./assets/build/icon.icns",
      "asar": true
    },
    "win": {
      "target": "portable",
      "icon": "./assets/build/icon.ico",
      "publisherName": "Yuri Tancredi"
    },
    "linux": {
      "target": "AppImage",
      "executableName": "LetMeSeo",
      "category": "Development",
      "description": "Analyze site SEO"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/git-yuritancredi/LetMeSEO.git"
  },
  "author": "Yuri Tancredi",
  "license": "CC0-1.0",
  "bugs": {
    "url": "https://github.com/git-yuritancredi/LetMeSEO/issues"
  },
  "homepage": "https://github.com/git-yuritancredi/LetMeSEO#readme",
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-react": "^7.9.4",
    "electron": "^8.2.5",
    "electron-builder": "^22.7.0",
    "gulp": "^4.0.2",
    "gulp-babel": "^8.0.0",
    "gulp-clean-css": "^4.3.0",
    "gulp-concat": "^2.6.1"
  },
  "dependencies": {
    "@material-ui/core": "^4.9.12",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.52",
    "fetch": "^1.1.0",
    "he": "^1.2.0",
    "lokijs": "^1.5.8",
    "node-html-parser": "^1.2.16",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  }
}
